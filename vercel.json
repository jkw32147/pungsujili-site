{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "http://www.pungsujili.com",
      "destination": "https://www.pungsujili.com",
      "statusCode": 308
    },
    {
      "source": "http://pungsujili.com",
      "destination": "https://www.pungsujili.com",
      "statusCode": 308
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, no-cache, must-revalidate, proxy-revalidate, max-age=0, s-maxage=0" },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" },
        { "key": "Surrogate-Control", "value": "no-store" }
      ]
    }
  ]
}

